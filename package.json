{
    "name": "lulo",
    "version": "4.0.0-beta3",
    "main": ".dist/index.js",
    "types": ".dist/index.d.ts",
    "scripts": {
        "clean": "rm -rf node_modules package-lock.json .dist .coverage",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json",
        "test:unit": "LOG_LEVEL=silent jest --config jest.config.json",
        "test": "npm run lint && npm run test:unit",
        "prepublishOnly": "npm test && npm run build"
    },
    "files": [
        ".dist/**/*"
    ],
    "dependencies": {
        "log4njs": "^3.0.2"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.114",
        "@types/jest": "29.5.1",
        "@types/node": "18.16.0",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "ts-jest": "29.1.0",
        "tslint": "6.1.3",
        "tslint-config-prettier": "1.18.0",
        "typescript": "5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/By-Nordenfelt-AB/lulo"
    },
    "keywords": [
        "AWS",
        "CloudFormation",
        "Custom Resources"
    ],
    "author": "Carl Nordenfelt <carl@bynordenfelt.com>",
    "license": "MIT",
    "issues": {
        "url": "https://github.com/By-Nordenfelt-AB/lulo/issues"
    },
    "homepage": "https://github.com/By-Nordenfelt-AB/lulo"
}
